    "concepts": [
      {
        "id": "math.c1",
        "name": "变量与代数式",
        "difficulty": 0.3,
        "prerequisites": [],
        "examples": [
          "用x表示未知数：买3支笔每支2元，总价=3×2=6；若每支x元，总价=3x",
          "代数式：2x+3表示“x的2倍再加3”"
        ],
        "exercises": [
          "将“一个数的3倍减5”用代数式表示",
          "当x=4时，计算2x+3的值"
        ]
      },
      {
        "id": "math.c2",
        "name": "代数式化简与代入",
        "difficulty": 0.4,
        "prerequisites": ["math.c1"],
        "examples": [
          "化简：3x+2x=5x",
          "代入：若x=2，则5x-1=9"
        ],
        "exercises": [
          "化简：4a-2a+7",
          "当a=-3时，计算4a-2a+7"
        ]
      },
      {
        "id": "math.c3",
        "name": "一次方程与解法（移项）",
        "difficulty": 0.5,
        "prerequisites": ["math.c1"],
        "examples": [
          "解方程：2x+3=11 → 2x=8 → x=4",
          "解方程：5x-2=3x+6 → 2x=8 → x=4"
        ],
        "exercises": [
          "解：3x+5=20",
          "解：7x-1=2x+14"
        ]
      },
      {
        "id": "math.c4",
        "name": "函数概念（对应与变量）",
        "difficulty": 0.6,
        "prerequisites": ["math.c3"],
        "examples": [
          "y=2x：x每变化1，y变化2",
          "将“路程=速度×时间”写成函数：s=vt"
        ],
        "exercises": [
          "判断：y=1/x在x=0时是否有意义？",
          "若y=2x+1，求x=3时的y"
        ]
      },
      {
        "id": "math.c5",
        "name": "一次函数与斜率直观",
        "difficulty": 0.6,
        "prerequisites": ["math.c4"],
        "examples": [
          "y=kx+b：k决定倾斜程度，b决定与y轴交点",
          "例：y=2x+1比y=x+1更“陡”"
        ],
        "exercises": [
          "比较y=3x与y=x的斜率大小",
          "若y=-2x+4，求x=1时y，并说明趋势（增/减）"
        ]
      },

      {
        "id": "cs.c1",
        "name": "变量与数据类型（直观）",
        "difficulty": 0.3,
        "prerequisites": [],
        "examples": [
          "x = 3 表示把数字3存到变量x",
          "name = \"Li\" 存字符串到name"
        ],
        "exercises": [
          "写出两个变量：一个存年龄，一个存姓名",
          "解释：变量和常量有什么区别？"
        ]
      },
      {
        "id": "cs.c2",
        "name": "表达式与输入输出",
        "difficulty": 0.4,
        "prerequisites": ["cs.c1"],
        "examples": [
          "输入a,b，输出a+b",
          "将摄氏度转华氏：f = c*9/5 + 32"
        ],
        "exercises": [
          "写伪代码：输入半径r，输出圆面积",
          "解释：为什么要把计算拆成步骤？"
        ]
      },
      {
        "id": "cs.c3",
        "name": "条件判断：if/else",
        "difficulty": 0.45,
        "prerequisites": ["cs.c2"],
        "examples": [
          "如果分数>=60就“及格”，否则“不及格”",
          "判断一个数是否为偶数：n%2==0"
        ],
        "exercises": [
          "写伪代码：判断输入n是正/负/零",
          "写伪代码：比较a和b，输出较大者"
        ]
      },
      {
        "id": "cs.c4",
        "name": "循环：for/while 与计数思想",
        "difficulty": 0.6,
        "prerequisites": ["cs.c2"],
        "examples": [
          "for i in 1..10：累加求和",
          "while 条件：直到输入正确为止"
        ],
        "exercises": [
          "写伪代码：求1到n的和",
          "写伪代码：统计一串输入里有多少个偶数"
        ]
      },
      {
        "id": "cs.c5",
        "name": "网络分层概念（直观）",
        "difficulty": 0.55,
        "prerequisites": ["cs.c1"],
        "examples": [
          "类比寄快递：写地址/打包/运输/签收",
          "OSI七层：先记住“应用/传输/网络/链路”的直观分工"
        ],
        "exercises": [
          "举例：浏览器访问网页涉及哪两层最明显？为什么？",
          "解释：分层的好处是什么（可替换、解耦）？"
        ]
      }
    ]
  },

